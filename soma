programa {
  funcao inicio() {
    inteiro numero_1
    inteiro numero_2
    inteiro resultado

    escreva ("Digite o primeiro numero: ")
    leia(numero_1)

     escreva ("digite o segundo numero: ")
     leia (numero_2)

     resultado = numero_1 + numero_2
     
     escreva ("A soma dos números é:  ", resultado)
  }
    
}
